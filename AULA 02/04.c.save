#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    // Semente para o gerador de números aleatórios
    int n;
    int m = 1024;
    float f;
    srand(time(NULL));
    n = rand();
    n = n%m;
    f = (float) n;
    f = f/m;
    f = f-0.5;
    f = f/0.5;

    printf("%f\n", f);
    return 0;

    // Gera um número aleatório entre 0 e 1

}
